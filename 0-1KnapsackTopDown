#include <bits/stdc++.h>
using namespace std;


int check(vector<int>w, vector<int>v, int C, int n){
    int t[n+1][C+1];
    for(int i=0;i<=n;i++){
        for(int j=0;j<=C;j++){
            if(i==0 || j==0)
                t[i][j]=0;
            else if(w[n-1]>C)
                t[i][j]= t[i-1][j];
             else{
                t[i][j]= max(t[i-1][j],v[i-1]+t[i-1][j-w[i-1]]);
             }
        }
    }
    
    return t[n][C];
}

int main(){
    
    vector<int>w;
    vector<int>v;
    int C,n,a;
    cout<<"Enter length of weight and value array:";
    cin>>n;
    cout<<"Enter values for weight array: ";
    for(int i=0;i<n;i++){
        cin>>a;
        w.push_back(a);
    }
    cout<<"Enter values for value array: ";
    for(int i=0;i<n;i++){
        cin>>a;
        v.push_back(a);
    }
    cout<<"Enter maximum capacity: ";
    cin>>C;
    
    cout<<check(w,v,C,n);
}
